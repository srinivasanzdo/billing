{
  "name": "electron-typescrit-express-boilerplate",
  "productName": "Electron Typescript Express Boilerplate",
  "description": "Starter for your Electron application including typescript express sequelize mysql",
  "version": "0.0.0",
  "author": "Mr. Sivakavi <sivakavi63@gmail.com>",
  "license": "MIT",
  "main": "app/background.js",
  "build": {
    "appId": "com.sivakavi.electron-boilerplate",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha app/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/electron": "^1.4.33",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.7",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "connect-flash": "^0.1.1",
    "ejs": "^2.5.7",
    "ejs-locals": "^1.0.2",
    "electron-prebuilt": "^1.4.13",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "fs-jetpack": "^0.10.2",
    "mysql2": "^1.4.2",
    "nodemailer": "^4.2.0",
    "reflect-metadata": "^0.1.10",
    "run-sequence": "^1.2.2",
    "sequelize": "^4.13.0",
    "sequelize-cli": "^3.0.0",
    "sequelize-typescript": "^0.3.1",
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "^1.4.7",
    "electron-builder": "^8.6.0",
    "electron-mocha": "^3.0.0",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-clean": "^0.3.2",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-typescript": "^3.1.5",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "path": "^0.12.7",
    "rollup": "^0.36.3",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.2",
    "spectron": "^3.3.0"
  }
}
